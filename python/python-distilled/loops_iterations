
s = [(1, 2), (3, 4), (5, 6)]
s_2 = [(1, 2, 3), (4, 5), (6, 7, 8)]

# Unpacking
for a, b in s:
    print(a, b) # 1 2, 3 4, 5 6

# Unpacking with variable number of elements
for a, b, *c in s_2:
    print(a, b, c) # 1 2 [3], 4 5 [], 6 7 [8]
    
 # Unpacking throwing away elements
for a, _ in s:
    print(a) # 1, 3, 5 